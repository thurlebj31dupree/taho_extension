{
  "compilerOptions": {
    "target": "esnext",
    "declaration": false,
    "moduleResolution": "node2",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "strict": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "noImplicitOverride": true
  },
  "exclude": ["dev-utils", "ci"]
}
